@prefix rr:   <http://www.w3.org/ns/r2rml#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ex:   <http://example.com/base/> .
@prefix semunit: <http://example.com/base/semanticunits/> .

<#specifiedOutputOfStatementUnit_Dataset>
  a rr:TriplesMap ;

  # One logical table via SQL that yields BOTH ids:
  rr:logicalTable [
    rr:sqlQuery """
      -- Dataset  Publication pairs derived from be_links
      -- (handles either direction in links_source/types vs links_target/types)
      WITH lp AS (
        SELECT
          CASE WHEN l.links_source_types = 'Dataset'     THEN l.links_source_ids
               WHEN l.links_target_types = 'Dataset'     THEN l.links_target_ids
          END AS dataset_id,
          CASE WHEN l.links_source_types = 'Publication' THEN l.links_source_ids
               WHEN l.links_target_types = 'Publication' THEN l.links_target_ids
          END AS pub_id,
          l.links_reference_types
        FROM be_links l
        WHERE
          (l.links_source_types IN ('Dataset','Publication')
           AND l.links_target_types IN ('Dataset','Publication'))
      )
      SELECT DISTINCT
        d.id   AS dataset_id,
        p.id   AS pub_id
      FROM lp
      JOIN be_dataset_metadata     d ON d.id = lp.dataset_id
      JOIN be_publication_metadata p ON p.id = lp.pub_id
      -- OPTIONAL: restrict to particular link kinds that denote
      -- publication  dataset (specified output):
      -- WHERE lp.links_reference_types IN ('usesDataset','documents','isDocumentedBy')
    """
  ] ;

  # Subject is the Dataset-specific SU
  rr:subjectMap [
    rr:template "http://example.com/base/semunit/specifiedOutputOfStatementUnit/Dataset_{dataset_id}" ;
    rr:termType rr:IRI
  ] ;

  # Types
  rr:predicateObjectMap [
    rr:predicate rdf:type ;
    rr:objectMap  [ rr:constant semunit:specifiedOutputOfStatementUnit ] ;
    #rr:graphMap   [ rr:template "http://example.com/base/semunit/specifiedOutputOfStatementUnit/Dataset_{dataset_id}" ] ;
  ] ;

  rr:predicateObjectMap [
    rr:predicate rdf:type ;
    rr:objectMap  [ rr:constant semunit:assertionalStatementUnit ] ;
    #rr:graphMap   [ rr:template "http://example.com/base/semunit/specifiedOutputOfStatementUnit/Dataset_{dataset_id}" ] ;
  ] ;

  # Link this SU to the dataset resource itself
  rr:predicateObjectMap [
  rr:predicate ex:semanticUnitSubject ;
  rr:objectMap [
    rr:template "http://example.com/base/Dataset_{dataset_id}" ;  # must match <#Dataset_id> template exactly
    rr:termType rr:IRI
  ] ;
  #rr:graphMap [ rr:template "http://example.com/base/semunit/specifiedOutputOfStatementUnit/Dataset_{dataset_id}" ] ;
] .


