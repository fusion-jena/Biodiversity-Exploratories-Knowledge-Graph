<#NamedIndividualIdentificationUnit_Dataset>
  a rr:TriplesMap ;
  rr:logicalTable   [ rr:tableName "be_dataset_metadata" ] ;
  rr:subjectMap     [
    rr:template    "http://example.com/base/semunit/namedindividualidentificationunit/Dataset_{id}" ;
    ##rr:graphMap    [ rr:template "http://example.com/base/semunit/namedindividualidentificationunit/Dataset_{id}" ] ;
  ] ;
  rr:predicateObjectMap [
    rr:predicate rdf:type ;
    rr:objectMap  [ rr:constant semunit:namedindividualidentificationunit] ;
  ] ; 

  rr:predicateObjectMap [
    rr:predicate ex:semanticUnitSubject ;
    rr:objectMap  [ rr:parentTriplesMap <#Dataset_id> ] ;
  ] .

<#Dataset_id>
a rr:TriplesMap ;

    rr:logicalTable [ rr:tableName "be_dataset_metadata"] ;

    rr:subjectMap [
        rr:template "http://example.com/base/Dataset_{id}" ;
        rr:class iao:dataset ;
        rr:termType rr:IRI;
    ] ;

    rr:predicateObjectMap [
        rr:predicate rdf:type ;
        rr:objectMap [
            rr:template "iao:dataset" ;
            rr:termType rr:IRI ;
        ] ;
        rr:graphMap  [
            rr:template "http://example.com/base/semunit/namedindividualidentificationunit/Dataset_{id}"
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate rdfs:label ;
        rr:objectMap [
            rr:template "Dataset {id}" ;
            rr:language "en" ;
        ] ;
        rr:graphMap  [
            rr:template "http://example.com/base/semunit/namedindividualidentificationunit/Dataset_{id}"
        ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcterms:title ;  
    rr:objectMap [
      rr:column "title" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcterms:abstract ;  
    rr:objectMap [
      rr:column "abstract" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate schema:keywords ;  
    rr:objectMap [
      rr:column "keywords" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcterms:format ;  
    rr:objectMap [
      rr:column "contenttype" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate omo:acronym ;  
    rr:objectMap [
      rr:template "acronym: {acronym}; meaning: {meaning}" ;
      rr:termType rr:Literal ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcterms:identifier ;  
    rr:objectMap [
      rr:column   "doi" ;
      ] ;
    rr:objectMap [
      rr:column   "identifier" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate schema:url ;  
    rr:objectMap [
      rr:column   "url" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcterms:conformsTo ;  
    rr:objectMap [
      rr:column   "metadata_schema" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcterms:license ;  
    rr:objectMap [
      rr:column   "license" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcat:startDate ;  
    rr:objectMap [
      rr:column   "start_date" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcat:endDate ;  
    rr:objectMap [
      rr:column   "end_date" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate obi:isSpecifiedOutputOf ;  
    rr:objectMap [
      rr:template "http://example.com/base/Project/{project_hash}" ;
      rr:termType rr:IRI;
      ] ;
      #rr:graphMap  [
      #      rr:template "http://example.com/base/semunit/specifiedOutputOfStatementUnit/Dataset_{id}"  ;
      #  ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcterms:contributor ;  
    rr:objectMap [
      rr:template "http://example.com/base/MetadataCreator/{metadata_creator_hash}" ;
      rr:termType rr:IRI;
      ] ;
    rr:objectMap [
      rr:template "http://example.com/base/ProjectLeader/{project_leader_hash}" ;
      rr:termType rr:IRI;
      ] ;
      rr:objectMap [
      rr:template "http://example.com/base/ContactPerson/{contact_person_hash}" ;
      rr:termType rr:IRI;
      ] ;
    rr:objectMap [
      rr:template "http://example.com/base/DataCreator/{data_creator_hash}" ;
      rr:termType rr:IRI;
      ] ;
      rr:objectMap [
      rr:template "http://example.com/base/DataCollector/{data_collector_hash}" ;
      rr:termType rr:IRI;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate schema:funder ;  
    rr:objectMap [
      rr:template "http://example.com/base/Funder/{funder_hash}" ;
      rr:termType rr:IRI;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate iao:isAbout ;  
    rr:objectMap [
      rr:template "http://example.com/base/BioticDataType/{bioticdatatypeclass}" ;
      rr:termType rr:IRI;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate iao:obi:isSpecifiedOutput ;  
    rr:objectMap [
      rr:template "http://example.com/base/DataPreparation_{id}" ;
      rr:termType rr:IRI;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate iao:obi:hasSpecifiedOutput ;  
    rr:objectMap [
      rr:template "http://example.com/base/DataAnalysis_{id}" ;
      rr:termType rr:IRI;
      ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate ex:dataCitation ;
        rr:objectMap [
            rr:column "datareferences" ;
            rr:language "en" ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate rdfs:comment ;
        rr:objectMap [
            rr:column "importantnotes" ;
            rr:language "en" ;
        ] ;
        rr:objectMap [
            rr:column "additional_information" ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate opmi:hasStatusContent ;
        rr:objectMap [
            rr:column "dataprocessingstatus" ;
            rr:language "en" ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate schema:citation ;
        rr:objectMap [
            rr:column "literaturereferences" ;
            rr:language "en" ;
        ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate ex:dataUploadStatus ;  
    rr:objectMap [
      rr:column   "datauploadstatus" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcat:inCatalog ;  
    rr:objectMap [
      rr:template "http://example.com/base/Repository/{name_of_repository_hash}" ;
      rr:termType rr:IRI;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate obi:isSpecifiedOutputOf ;  
    rr:objectMap [
      rr:template "http://example.com/base/Project/Core/{projects_core_hash}" ;
      rr:termType rr:IRI;
      ] ;
    ] .

