  ] .

<authorsAndRolesCompoundUnit_Publication>
  a rr:TriplesMap ;
  rr:logicalTable   [ rr:tableName "be_publication_metadata" ] ;
  rr:subjectMap     [
    rr:template    "http://example.com/base/semunit/authorsAndRolesCompoundUnit/Publication_{id}" ;
    rr:graphMap    [ rr:template "http://example.com/base/semunit/authorsAndRolesCompoundUnit/Publication_{id}" ] ;
  ] ;
  rr:predicateObjectMap [
    rr:predicate rdf:type ;
    rr:objectMap  [ rr:constant semunit:authorsAndRolesCompoundUnit] ;
  ] ;
  rr:predicateObjectMap [
    rr:predicate rdf:type ;
    rr:objectMap  [ rr:constant semunit:compoundUnit] ;
  ] ; 

  rr:predicateObjectMap [
        rr:predicate semunit:hasAssociatedSemanticUnit ;
        rr:objectMap [
            rr:template "http://example.com/base/semunit/firstAuthorRoleStatementUnit/Role/FirstAuthor_{id}" ;
            rr:termType rr:IRI ;
        ] ;
    ] ;  

  rr:predicateObjectMap [
        rr:predicate semunit:hasAssociatedSemanticUnit ;
        rr:objectMap [
            rr:template "http://example.com/base/semunit/coAuthorRoleStatementUnit/Role/CoAuthor_{id}" ;
            rr:termType rr:IRI ;
        ] ;
    ] ;   

  rr:predicateObjectMap [
        rr:predicate semunit:hasAssociatedSemanticUnit ;
        rr:objectMap [
            rr:template "http://example.com/base/semunit/creatorStatementUnit/Publication_{id}" ;
            rr:termType rr:IRI ;
        ] ;
    ] ;   

  rr:predicateObjectMap [
    rr:predicate ex:semanticUnitSubject ;
    rr:objectMap  [ rr:parentTriplesMap <#Publication_id> ] ;
  ] .  

<#Publication_id>
a rr:TriplesMap ;

    rr:logicalTable [ rr:tableName "be_publication_metadata"] ;

    rr:subjectMap [
        rr:template "http://example.com/base/Publication_{id}" ;
        rr:class iao:publication ;
        rr:termType rr:IRI;
    ] ;

    rr:predicateObjectMap [
        rr:predicate rdf:type ;
        rr:objectMap [
            rr:template "iao:publication" ;
            rr:termType rr:IRI ;
        ] ;
        rr:graphMap  [
            rr:template "http://example.com/base/semunit/namedindividualidentificationunit/Publication_{id}" ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate rdf:type ;
        rr:objectMap [
            rr:template "fabio:{typeofpublication}" ;
            rr:termType rr:IRI ;
        ] ;
        rr:graphMap  [
            rr:template "http://example.com/base/semunit/namedindividualidentificationunit/Publication_{id}" ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate rdfs:label ;
        rr:objectMap [
            rr:template "Publication {id}" ;
            rr:language "en" ;
        ] ;
        rr:graphMap  [
            rr:template "http://example.com/base/semunit/namedindividualidentificationunit/Publication_{id}" ;
        ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate obi:hasPart ;  
    rr:objectMap [
      rr:template "http://example.com/base/Title_{id}" ;
      rr:termType rr:IRI ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate obi:hasPart ;  
    rr:objectMap [
      rr:template "http://example.com/base/Summary_{id}" ;
      rr:termType rr:IRI ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate obi:hasPart ;  
    rr:objectMap [
      rr:template "http://example.com/base/Keywords_{id}" ;
      rr:termType rr:IRI ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcterms:bibliographicCitation ;  
    rr:objectMap [
      rr:column   "citation" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate fabio:hasPublicationYear ;  
    rr:objectMap [
      rr:column   "year" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcterms:status ;  
    rr:objectMap [
      rr:column   "status" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcterms:identifier ;  
    rr:objectMap [
      rr:column   "doi" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate schema:url ;  
    rr:objectMap [
      rr:column   "url" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate schema:isAccessibleForFree ;  
    rr:objectMap [
      rr:column   "openaccess" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcterms:publisher ;  
    rr:objectMap [
      rr:template "http://example.com/base/Publisher/{publisher}" ;
      rr:termType rr:IRI ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcterms:conformsTo ;  
    rr:objectMap [
      rr:column   "metadataschema" ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcterms:creator ;  
    rr:objectMap [
      rr:template  "http://example.com/base/Author/{firstauthor_hash}" ;
      rr:termType rr:IRI ;
      ] ;
      rr:graphMap  [
            rr:template "http://example.com/base/semunit/creatorStatementUnit/Publication_{id}" ;
        ] ;
      rr:graphMap  [
            rr:template "http://example.com/base/semunit/authorsAndRolesCompoundUnit/Publication_{id}" ;
        ] ;  
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcterms:creator ;  
    rr:objectMap [
      rr:template  "http://example.com/base/Author/{co_authors_hash}" ;
      rr:termType rr:IRI ;
      ] ;
      rr:graphMap  [
            rr:template "http://example.com/base/semunit/creatorStatementUnit/Publication_{id}" ;
        ] ;
      rr:graphMap  [
            rr:template "http://example.com/base/semunit/authorsAndRolesCompoundUnit/Publication_{id}" ;
        ] ;  
    ] ;

    rr:predicateObjectMap [
    rr:predicate foaf:mbox ;  
    rr:objectMap [
      rr:column "correspondingemail";
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate dcterms:contributor ;  
    rr:objectMap [
      rr:template "http://example.com/base/Institution/{institutions_hash}";
      rr:termType rr:IRI ;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate obi:isSpecifiedOutputOf ;  
    rr:objectMap [
      rr:template "http://example.com/base/Project/{project_hash}_{id}" ;
      rr:termType rr:IRI;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate obi:isSpecifiedOutputOf ;  
    rr:objectMap [
      rr:template "http://example.com/base/Project/Core/{projects_core_hash}_{id}" ;
      rr:termType rr:IRI;
      ] ;
    ] ;

    rr:predicateObjectMap [
    rr:predicate iao:isAbout ;  
    rr:objectMap [
      rr:template "http://example.com/base/Study/{laboratory}_{id}" ;
      rr:termType rr:IRI;
      ] ;
      rr:objectMap [
      rr:template "http://example.com/base/Study/{field}_{id}" ;
      rr:termType rr:IRI;
      ] ;
      rr:objectMap [
      rr:template "http://example.com/base/Study/{review}_{id}" ;
      rr:termType rr:IRI;
      ] ;
      rr:objectMap [
      rr:template "http://example.com/base/BioticDataType/{singlespecies}" ;
      rr:termType rr:IRI;
      ] ;
      rr:objectMap [
      rr:template "http://example.com/base/BioticDataType/{multiplespecies}" ;
      rr:termType rr:IRI;
      ] ;
      rr:objectMap [
      rr:template "http://example.com/base/BioticDataType/{genetic}" ;
      rr:termType rr:IRI;
      ] ;
      rr:objectMap [
      rr:template "http://example.com/base/BioticDataType/{totalabundance}" ;
      rr:termType rr:IRI;
      ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate obi:partOf ;
        rr:objectMap [
            rr:template "http://example.com/base/BE_Infrastructure_{infrastructureclass}" ;
            rr:termType rr:IRI ;
        ] ;
        rr:graphMap  [
            rr:template "http://example.com/base/semunit/parthoodStatementUnit/Publication_{id}" ;
        ] ;
        rr:graphMap    [ rr:template "http://example.com/base/semunit/Infrastructure_ProcessAndService_Environment_Publication_Link_Projects_CompoundUnit/Publication_{id}" ] ;
    ] .

